{
  "cli": {
    "version": ">= 7.1.0",
    "appVersionSource": "local"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal",
      "ios": {
        "resourceClass": "m-medium"
      }
    },
    "preview": {
      "distribution": "internal",
      "ios": {
        "resourceClass": "m-medium"
      }
    },
    "preview-simulator": {
      "ios": {
        "simulator": true,
        "resourceClass": "m-medium"
      },
      "env": {
        "GOOGLE_SERVICE_INFO_PLIST": "@file:GOOGLE_SERVICE_INFO_PLIST"
      }
    },
    "production": {
      "ios": {
        "resourceClass": "m-medium"
      },
      "env": {
        "EXPO_PUBLIC_FORCE_PRODUCTION": "true",
        "EXPO_PUBLIC_USE_MOCK": "false"
      },
      "hooks": {
        "eas": {
          "build": {
            "onStart": {
              "scripts": ["node ./scripts/fix-safe-area-ios-build.js"]
            }
          }
        }
      }
    }
  },
  "submit": {
    "production": {
      "ios": {
        "appleId": "chris@grumpylemon.com",
        "ascAppId": "6746467670",
        "appleTeamId": "87699862N5"
      }
    }
  }
}
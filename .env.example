# Firebase Configuration
# Copy this file to .env and fill in your Firebase project values
# You can find these values in your Firebase Console > Project Settings

EXPO_PUBLIC_FIREBASE_API_KEY=your-api-key-here
EXPO_PUBLIC_FIREBASE_AUTH_DOMAIN=your-auth-domain.firebaseapp.com
EXPO_PUBLIC_FIREBASE_PROJECT_ID=your-project-id
EXPO_PUBLIC_FIREBASE_STORAGE_BUCKET=your-storage-bucket.appspot.com
EXPO_PUBLIC_FIREBASE_MESSAGING_SENDER_ID=your-sender-id
EXPO_PUBLIC_FIREBASE_APP_ID=your-app-id

# Feature Flags
# Set to 'true' to force mock implementation (useful for testing)
# Leave unset or 'false' to use real Firebase on supported platforms
EXPO_PUBLIC_USE_MOCK=false